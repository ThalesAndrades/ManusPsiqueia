# ManusPsiqueia Environment Variables Template
# Copy this file to .env and fill in your actual values

# ========================================
# Development Environment
# ========================================

# Stripe Configuration (Development)
STRIPE_PUBLISHABLE_KEY_DEV=pk_test_your_development_key_here
STRIPE_SECRET_KEY_DEV=sk_test_your_development_key_here

# Supabase Configuration (Development)
SUPABASE_URL_DEV=https://your-dev-project.supabase.co
SUPABASE_ANON_KEY_DEV=your_development_anon_key_here
SUPABASE_SERVICE_ROLE_KEY_DEV=your_development_service_role_key_here

# OpenAI Configuration (Development)
OPENAI_API_KEY_DEV=sk-your_development_openai_key_here

# ========================================
# Staging Environment
# ========================================

# Stripe Configuration (Staging)
STRIPE_PUBLISHABLE_KEY_STAGING=pk_test_your_staging_key_here
STRIPE_SECRET_KEY_STAGING=sk_test_your_staging_key_here

# Supabase Configuration (Staging)
SUPABASE_URL_STAGING=https://your-staging-project.supabase.co
SUPABASE_ANON_KEY_STAGING=your_staging_anon_key_here
SUPABASE_SERVICE_ROLE_KEY_STAGING=your_staging_service_role_key_here

# OpenAI Configuration (Staging)
OPENAI_API_KEY_STAGING=sk-your_staging_openai_key_here

# ========================================
# Production Environment
# ========================================

# Stripe Configuration (Production)
STRIPE_PUBLISHABLE_KEY_PROD=pk_live_your_production_key_here
STRIPE_SECRET_KEY_PROD=sk_live_your_production_key_here

# Supabase Configuration (Production)
SUPABASE_URL_PROD=https://your-prod-project.supabase.co
SUPABASE_ANON_KEY_PROD=your_production_anon_key_here
SUPABASE_SERVICE_ROLE_KEY_PROD=your_production_service_role_key_here

# OpenAI Configuration (Production)
OPENAI_API_KEY_PROD=sk-your_production_openai_key_here

# ========================================
# Team Configuration
# ========================================

# Apple Developer Team ID (required for code signing)
DEVELOPMENT_TEAM_ID=YOUR_TEAM_ID_HERE

# ========================================
# App Configuration
# ========================================

# App Store Connect Configuration
APP_STORE_CONNECT_API_KEY_ID=your_api_key_id_here
APP_STORE_CONNECT_ISSUER_ID=your_issuer_id_here
APP_STORE_CONNECT_API_KEY_PATH=/path/to/your/api/key.p8

# Push Notification Configuration
APNS_KEY_ID=your_apns_key_id_here
APNS_TEAM_ID=your_team_id_here
APNS_KEY_PATH=/path/to/your/apns/key.p8

# ========================================
# Feature Flags
# ========================================

# Enable/Disable features per environment
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_MENU=false
ENABLE_LOGGING=true

# ========================================
# Security Configuration
# ========================================

# Certificate pinning
CERTIFICATE_PINNING_ENABLED=true
NETWORK_SECURITY_LEVEL=HIGH

# API Rate Limiting
API_RATE_LIMIT_ENABLED=true
API_RATE_LIMIT_REQUESTS_PER_MINUTE=60

# ========================================
# Third-party Integrations
# ========================================

# Analytics (if using)
ANALYTICS_API_KEY=your_analytics_key_here

# Crash Reporting (if using)
CRASH_REPORTING_API_KEY=your_crash_reporting_key_here

# ========================================
# Development Tools
# ========================================

# SwiftLint configuration
SWIFTLINT_ENABLED=true

# Testing configuration
ENABLE_UI_TESTING=true
ENABLE_UNIT_TESTING=true

# Xcode Cloud configuration
XCODE_CLOUD_ENABLED=true
CI_WORKFLOW=Development

# ========================================
# Notes
# ========================================

# 1. Never commit the .env file with real values to version control
# 2. Use different values for each environment (dev, staging, prod)
# 3. Ensure all team members have their own .env file
# 4. For production, use secure environment variable management
# 5. Test API connectivity after setting up environment variables